{
    "name": "@automapper/pojos",
    "version": "8.8.1",

    "main": "./index.cjs.js",
    "module": "./index.js",
    "types": "./src/index.d.ts",
    "exports": {
        ".": {
            "types": "./src/index.d.ts",
            "import": {
                "types": "./src/index.d.ts",
                "default": "./index.esm.js"
            },
            "require": {
                "types": "./src/index.d.ts",
                "default": "./index.cjs.js"
            },
            "default": "./index.esm.js"
        }
    },
    "sideEffects": false,
    "engines": {
        "node": ">=16.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "@automapper/core": "workspace:*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nartc/mapper/tree/main/packages/pojos"
    },
    "author": {
        "name": "Chau Tran",
        "email": "nartc7789@gmail.com",
        "url": "https://nartc.me"
    },
    "description": "AutoMapper TypeScript POJOs plugin",
    "keywords": [
        "typescript",
        "automapper",
        "mapper",
        "nx"
    ],
    "license": "MIT",
    "files": [
        "**/*"
    ]
}
