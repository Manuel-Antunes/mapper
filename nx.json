{
  "extends": "nx/presets/core.json",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "default": {
    "runner": "nx/tasks-runners/default",
    "options": {
      "cacheableOperations": [
        "build",
        "lint",
        "test",
        "e2e"
      ],
      "cacheDirectory": "/tmp/nx-cache"
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ],
    "prod": [
      "!{projectRoot}/**/*.spec.ts"
    ]
  },
  "targetDefaults": {
    "test": {
      "inputs": [
        "default",
        "^prod"
      ],
      "cache": true
    },
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json"
      ],
      "cache": true
    },
    "build": {
      "cache": true
    },
    "e2e": {
      "cache": true
    },
    "package": {
      "cache": true
    },
    "package-lib": {
      "cache": true
    }
  },
  "pluginsConfig": {
    "@nx/js": {
      "analyzeSourceFiles": true
    }
  },
  "nxCloudAccessToken": "MjMzYTA3OWMtMjQ1MS00YzFhLWExYTYtODMzMTM5MmQxZmE1fHJlYWQtd3JpdGU=",
  "useInferencePlugins": false,
  "defaultBase": "main"
}
