{
    "tasksRunnerOptions": {
        "default": {
            "runner": "nx-cloud",
            "options": {
                "cacheableOperations": [
                    "build",
                    "lint",
                    "test",
                    "e2e",
                    "package",
                    "package-lib"
                ],
                "accessToken": "MjMzYTA3OWMtMjQ1MS00YzFhLWExYTYtODMzMTM5MmQxZmE1fHJlYWQtd3JpdGU="
            }
        }
    },
    "extends": "nx/presets/core.json",
    "npmScope": "automapper",
    "affected": {
        "defaultBase": "main"
    },
    "cli": {
        "defaultCollection": "@nx/workspace"
    },
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "namedInputs": {
        "default": ["{projectRoot}/**/*"],
        "prod": ["!{projectRoot}/**/*.spec.ts"]
    },
    "targetDefaults": {
        "test": {
            "inputs": ["default", "^prod"]
        },
        "lint": {
            "inputs": ["default", "{workspaceRoot}/.eslintrc.json"]
        }
    },
    "pluginsConfig": {
        "@nx/js": {
            "analyzeSourceFiles": true
        }
    }
}
